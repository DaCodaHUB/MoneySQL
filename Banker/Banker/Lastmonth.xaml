<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banker"
        xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" x:Class="Banker.Lastmonth"
        mc:Ignorable="d"
        Title="Last Month" Height="428.4" Width="481.6">
    <Grid>

        <toolkit:Chart  Name="lineChart" Title="Last Month Chart" 

		HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.423,0.09" Height="400" Width="476">
            <toolkit:LineSeries  DependentValuePath="Value" 

		IndependentValuePath="Key" ItemsSource="{Binding}" 

		IsSelectionEnabled="True" Title="USD"/>
        </toolkit:Chart>


    </Grid>
</Window>
