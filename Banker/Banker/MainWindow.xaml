<Window x:Class="Banker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banker"
        mc:Ignorable="d"
        Title="Banker" Height="160" Width="330">
    <Grid>
        <Label Content="Username" HorizontalAlignment="Left" Margin="28,21,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="28,52,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="109,23,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="109,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <Button Content="Sign In" HorizontalAlignment="Left" Margin="109,92,0,0" VerticalAlignment="Top" Width="75" Click="SignIn_CLick"/>
        <Button Content="Sign Up" HorizontalAlignment="Left" Margin="201,92,0,0" VerticalAlignment="Top" Width="75" Click="SignUp_Click"/>

    </Grid>
</Window>
