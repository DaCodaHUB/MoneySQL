<Window x:Class="Banker.SignUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banker"
        mc:Ignorable="d"
        Title="SignUp" Height="194" Width="307.199">
    <Grid>
        <Label Content="Email" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name ="email" HorizontalAlignment="Left" Height="23" Margin="89,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <TextBox Name ="usernameTxt" HorizontalAlignment="Left" Height="23" Margin="89,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,75,0,0" VerticalAlignment="Top"/>
        <TextBox Name="passwordTxt" HorizontalAlignment="Left" Height="23" Margin="89,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>
        <Button Content="Sign Up" HorizontalAlignment="Left" Margin="89,117,0,0" VerticalAlignment="Top" Width="112" Click="SignUp_OnClick"/>

    </Grid>
</Window>
